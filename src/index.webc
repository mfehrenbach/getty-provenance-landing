<!doctype html>
<html lang="en">
	<head>
		<meta charset="utf-8">
		<meta
			content="initial-scale=1, width=device-width, viewport-fit=cover"
			name="viewport"
		/>
		<title @text="page.title"></title>
		<link webc:import="assets/fonts.webc">
		<link
			href="assets/style.css"
			rel="stylesheet"
		>
		<script src="assets/script.js"></script>
		<!--- Assets are bundled inline here from the files. --->
		<style
			webc:keep
			@raw="getBundle('css')"
		></style>
		<script
			webc:keep
			@raw="getBundle('js')"
			type="module"
		></script>
	</head>
	<body>
		<h1 @text="page.title"></h1>
		<form :data-base-url="page.baseUrl">
			<label
				@text="page.searchLabel"
				for="search"
			>
			</label>
			<input
				:placeholder="page.searchPlaceholder"
				autocapitalize="off"
				autocomplete="off"
				autocorrect="off"
				id="search"
				name="search"
				required
				spellcheck="false"
				type="text"
			>
			<input
				:value="page.searchButton"
				type="submit"
			>
		</form>
		<h2 @text="page.exampleHeading"></h2>
		<a
			:href="page.baseUrl + page.exampleUrl"
			@text="page.exampleLabel"
		></a>
		<h2
			:data-api-url="page.apiUrl"
			@text="page.apiHeading"
		></h2>
		<aside>
			<span>.</span>
			<span>.</span>
			<span>.</span>
		</aside>
	</body>
</html>
